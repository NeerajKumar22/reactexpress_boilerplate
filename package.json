{
  "name": "reactexpress_boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for React and Express",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NeerajKumar22/reactexpress_boilerplate.git"
  },
  "author": "Neeraj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NeerajKumar22/reactexpress_boilerplate/issues"
  },
  "homepage": "https://github.com/NeerajKumar22/reactexpress_boilerplate#readme",
  "dependencies": {
    "concurrently": "^4.1.2",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  }
}
